# Base URL publique (sans slash final), ex: https://app.votre-domaine.com
VITE_PUBLIC_BASE_URL=

# Base path du site (inclure les slashes) ex: /mon-repo/ pour GitHub Pages
VITE_BASE=

# Identifiants SingPay (utiliser des secrets réels en prod)
VITE_SINGPAY_CLIENT_ID=
VITE_SINGPAY_CLIENT_SECRET=
VITE_SINGPAY_WALLET=
VITE_SINGPAY_DISBURSEMENT=

# Cloudinary
VITE_CLOUDINARY_CLOUD_NAME=
VITE_CLOUDINARY_UPLOAD_PRESET=

# (Optionnel) URL proxy paiement (Vercel/Netlify) si différent de la détection
VITE_PAYMENT_PROXY_URL=

# Prix mensuel de l'abonnement (XAF)
VITE_SUB_PRICE_XAF=5000

# Unsplash API (pour recherche automatique d'images de produits)
VITE_UNSPLASH_ACCESS_KEY= 